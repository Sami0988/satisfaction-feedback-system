                "responses": {
                    "200": {
                        "description": "A list of users"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Department": {
                "title": "Department",
                "required": [
                    "id",
                    "name",
                    "code",
                    "email",
                    "phone"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Finance"
                    },
                    "code": {
                        "type": "string",
                        "example": "FIN"
                    },
                    "email": {
                        "type": "string",
                        "example": "finance@company.com"
                    },
                    "phone": {
                        "type": "string",
                        "example": "+251912345678"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
